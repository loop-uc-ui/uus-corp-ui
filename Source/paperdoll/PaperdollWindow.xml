			<EventHandlers>
				<EventHandler event="OnInitialize" function="PaperdollWindow.onInitialize" />
				<EventHandler event="OnShutdown" function="PaperdollWindow.onShutdown" />
				<EventHandler event="OnLButtonDown" function="PaperdollWindow.OnMouseDrag" />
				<EventHandler event="OnMouseWheel" function="WindowUtils.Scale" />
				<EventHandler event="OnRButtonUp" function="PaperdollWindow.onRightClick" />
