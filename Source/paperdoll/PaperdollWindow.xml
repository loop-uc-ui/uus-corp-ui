<Interface xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../Interface.xsd">
	<Windows>
		<Window name="PaperdollWindow" movable="true" sticky="true" popable="true" layer="default">
			<Size>
				<AbsPoint x="240" y="372" />
			</Size>
			<EventHandlers>
				<EventHandler event="OnInitialize" function="PaperdollWindow.onInitialize" />
				<EventHandler event="OnShutdown" function="PaperdollWindow.onShutdown" />
				<EventHandler event="OnLButtonDown" function="PaperdollWindow.OnMouseDrag" />
				<EventHandler event="OnMouseWheel" function="WindowUtils.Scale" />
				<EventHandler event="OnRButtonUp" function="PaperdollWindow.onRightClick" />
			</EventHandlers>
			<Windows>

                <FullResizeImage name="$parentBackground" inherits="UO_Simple_Black_Background" layer="background">
                    <Anchors>
                        <Anchor point="topleft" relativePoint="topleft"/>
                        <Anchor point="bottomright" relativePoint="bottomright" relativeTo="$parentToggleInventory">
                            <AbsPoint x="16" y="12"/>
                        </Anchor>
                    </Anchors>
                </FullResizeImage>

                <FullResizeImage name="$parentFrame" inherits="UO_Simple_Frame" layer="background" frameonly="true">
                    <Anchors>
                        <Anchor point="topleft" relativePoint="topleft" relativeTo="$parentBackground"/>
                        <Anchor point="bottomright" relativePoint="bottomright" relativeTo="$parentBackground" />
                    </Anchors>
                </FullResizeImage>

                <Label name="$parentName" font="uo_classic_18" maxchars="64" textalign="center" wordwrap="true" autoresize="true">
                    <Size>
                        <AbsPoint x="192" y="70"/>
                    </Size>
                    <Anchors>
                        <Anchor point="topleft" relativePoint="topleft">
                            <AbsPoint x="12" y="12" />
						</Anchor>
                    </Anchors>
                    <Color r="206" g="217" b="242" a="255"/>
                </Label>

				<Button name="$parentItemSlotButton1" inherits="ItemGridSocketTemplate" id="1" >
					<Anchors>
						<Anchor point="bottomleft" relativePoint="topleft" relativeTo="$parentName">
							<AbsPoint x="0" y="12" />
						</Anchor>
					</Anchors>
				</Button>
				<Button name="$parentItemSlotButton2" inherits="ItemGridSocketTemplate" id="2" >
					<Anchors>
						<Anchor point="right" relativePoint="left" relativeTo="$parentItemSlotButton1"/>
					</Anchors>
				</Button>
				<Button name="$parentItemSlotButton3" inherits="ItemGridSocketTemplate" id="3" >
					<Anchors>
						<Anchor point="right" relativePoint="left" relativeTo="$parentItemSlotButton2" />
					</Anchors>
				</Button>
				<Button name="$parentItemSlotButton4" inherits="ItemGridSocketTemplate" id="4" >
					<Anchors>
						<Anchor point="right" relativePoint="left" relativeTo="$parentItemSlotButton3"/>
					</Anchors>
				</Button>
				<Button name="$parentItemSlotButton5" inherits="ItemGridSocketTemplate" id="5" >
					<Anchors>
						<Anchor point="bottomleft" relativePoint="topleft" relativeTo="$parentItemSlotButton1"/>
					</Anchors>
				</Button>
				<Button name="$parentItemSlotButton6" inherits="ItemGridSocketTemplate" id="6" >
					<Anchors>
						<Anchor point="right" relativePoint="left" relativeTo="$parentItemSlotButton5"/>
					</Anchors>
				</Button>
				<Button name="$parentItemSlotButton7" inherits="ItemGridSocketTemplate" id="7" >
					<Anchors>
						<Anchor point="right" relativePoint="left" relativeTo="$parentItemSlotButton6"/>
					</Anchors>
				</Button>
				<Button name="$parentItemSlotButton8" inherits="ItemGridSocketTemplate" id="8" >
					<Anchors>
						<Anchor point="right" relativePoint="left" relativeTo="$parentItemSlotButton7"/>
					</Anchors>
				</Button>
				<Button name="$parentItemSlotButton9" inherits="ItemGridSocketTemplate" id="9" >
					<Anchors>
						<Anchor point="bottomleft" relativePoint="topleft" relativeTo="$parentItemSlotButton5"/>
					</Anchors>
				</Button>
				<Button name="$parentItemSlotButton10" inherits="ItemGridSocketTemplate" id="10" >
					<Anchors>
						<Anchor point="right" relativePoint="left" relativeTo="$parentItemSlotButton9" />
					</Anchors>
				</Button>
				<Button name="$parentItemSlotButton11" inherits="ItemGridSocketTemplate" id="11" >
					<Anchors>
						<Anchor point="right" relativePoint="left" relativeTo="$parentItemSlotButton10"/>
					</Anchors>
				</Button>
				<Button name="$parentItemSlotButton12" inherits="ItemGridSocketTemplate" id="12">
					<Anchors>
						<Anchor point="right" relativePoint="left" relativeTo="$parentItemSlotButton11"/>
					</Anchors>
				</Button>
				<Button name="$parentItemSlotButton13" inherits="ItemGridSocketTemplate" id="13" >
					<Anchors>
						<Anchor point="bottomleft" relativePoint="topleft" relativeTo="$parentItemSlotButton9"/>
					</Anchors>
				</Button>
				<Button name="$parentItemSlotButton14" inherits="ItemGridSocketTemplate" id="14" >
					<Anchors>
						<Anchor point="right" relativePoint="left" relativeTo="$parentItemSlotButton13"/>
					</Anchors>
				</Button>
				<Button name="$parentItemSlotButton15" inherits="ItemGridSocketTemplate" id="15" >
					<Anchors>
						<Anchor point="right" relativePoint="left" relativeTo="$parentItemSlotButton14" />
					</Anchors>
				</Button>
				<Button name="$parentItemSlotButton16" inherits="ItemGridSocketTemplate" id="16" >
					<Anchors>
						<Anchor point="right" relativePoint="left" relativeTo="$parentItemSlotButton15"/>
					</Anchors>
				</Button>
				<Button name="$parentItemSlotButton17" inherits="ItemGridSocketTemplate" id="17" >
					<Anchors>
						<Anchor point="bottomleft" relativePoint="topleft" relativeTo="$parentItemSlotButton13"/>
					</Anchors>
				</Button>
				<Button name="$parentItemSlotButton18" inherits="ItemGridSocketTemplate" id="18" >
					<Anchors>
						<Anchor point="right" relativePoint="left" relativeTo="$parentItemSlotButton17"/>
					</Anchors>
				</Button>
				<Button name="$parentItemSlotButton19" inherits="ItemGridSocketTemplate" id="19" >
					<Anchors>
						<Anchor point="right" relativePoint="left" relativeTo="$parentItemSlotButton18"/>
					</Anchors>
				</Button>


				<Button name="$parentToggleInventory" textureScale="0.80" sticky="false" backgroundtexture="hud_image" highlighttexture="hud_image" layer="overlay" popable="false" draganddrop="true" >
					<Size>
						<AbsPoint x="48" y="48" />
					</Size>
					<Anchors>
						<Anchor point="right" relativePoint="left" relativeTo="$parentToggleCharacterSheet">
							<AbsPoint x="28" y="-4" />
						</Anchor>
					</Anchors>
					<TexCoords>
						 <Normal x="330" y="383" />
						 <NormalHighlit x="392" y="383" />
						 <Pressed x="392" y="383" />
						 <PressedHighlit x="392" y="383" />
					</TexCoords>
					<EventHandlers>
						<EventHandler event="OnLButtonUp" function="PaperdollWindow.ToggleInventoryWindow" />
						<EventHandler event="OnMouseOver" function="PaperdollWindow.OnMouseOverToggleInventory" />
						<EventHandler event="OnMouseOverEnd" function="ItemProperties.ClearMouseOverItem" />
					</EventHandlers>
				</Button>

				<Button name="$parentToggleCharacterSheet" textureScale="0.80" sticky="false" backgroundtexture="hud_image" highlighttexture="hud_image" layer="overlay" popable="false">
					<Size>
						<AbsPoint x="48" y="48" />
					</Size>
					<Anchors>
						<Anchor point="right" relativePoint="left" relativeTo="$parentToggleCharacterAbilities">
							<AbsPoint x="28" y="0" />
						</Anchor>
					</Anchors>
					<TexCoords>
						<Normal         x="115" y="688" />
						<NormalHighlit  x="115" y="628" />
						<Pressed        x="115" y="628" />
						<PressedHighlit x="115" y="628" />
						<Disabled       x="115" y="688" />
					</TexCoords>
					<EventHandlers>
						<EventHandler event="OnLButtonUp" function="PaperdollWindow.ToggleCharacterSheet" />
						<EventHandler event="OnMouseOver" function="PaperdollWindow.OnMouseOverToggleCharacterSheet" />
					</EventHandlers>
				</Button>

				<Button name="$parentToggleCharacterAbilities" textureScale="0.80" sticky="false" backgroundtexture="hud_image" highlighttexture="hud_image" layer="overlay" popable="false">
					<Size>
						<AbsPoint x="48" y="48" />
					</Size>
					<Anchors>
						<Anchor point="bottom" relativePoint="top" relativeTo="$parentItemSlotButton17">
							<AbsPoint x="0" y="12" />
						</Anchor>
					</Anchors>
					<TexCoords>
						<Normal         x="115" y="749" />
						<NormalHighlit  x="115" y="567" />
						<Pressed        x="115" y="567" />
						<PressedHighlit x="115" y="567" />
						<Disabled       x="115" y="749" />
					</TexCoords>
					<EventHandlers>
						<EventHandler event="OnLButtonUp" function="PaperdollWindow.ToggleCharacterAbilities" />
						<EventHandler event="OnMouseOver" function="PaperdollWindow.OnMouseOverToggleCharacterAbilities" />
					</EventHandlers>
				</Button>
			</Windows>
		</Window>
	</Windows>
</Interface>