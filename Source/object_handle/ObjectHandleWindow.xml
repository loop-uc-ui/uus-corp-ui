<Interface xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../Interface.xsd">
	<Windows>
		<Window name="ObjectHandleWindow" movable="false" layer="background">
			<Size>
				<AbsPoint x="128" y="36" />
			</Size>
            <EventHandlers>
                <EventHandler event="OnInitialize" function="ObjectHandleWindow.onInitialize" />
                <EventHandler event="OnShutdown" function="ObjectHandleWindow.onShutdown" />
                <EventHandler event="OnRButtonUp" function="ObjectHandleWindow.onRightClick" />
                <EventHandler event="OnLButtonDblClk" function="ObjectHandleWindow.onDoubleClick"/>
                <EventHandler event="OnMouseDrag" function="ObjectHandleWindow.onMouseDrag"/>
            </EventHandlers>
			<Windows>
                <FullResizeImage name="$parentWindowBackground" inherits="UO_Simple_Black_Background" alpha="0.75">
                    <Anchors>
                        <Anchor point="topleft" relativePoint="topleft" relativeTo="$parentName"/>
                        <Anchor point="bottomright" relativePoint="bottomright" relativeTo="$parentName"/>
                    </Anchors>
                </FullResizeImage>

                <Label name="$parentName" font="inter_medium_18" maxchars="64" textalign="center" handleinput="false" poppable="false" autoresizewidth="true">
                    <Size>
                        <AbsPoint x="128" y="36" />
                    </Size>
                    <Anchors>
                        <Anchor point="top" relativePoint="top" relativeTo="$parent"/>
                    </Anchors>
                    <Color r="255" g="255" b="255" a="255" />
                </Label>
			</Windows>
		</Window>
	</Windows>
</Interface>
