<Interface xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../Interface.xsd">
    <Windows>
        <Window name="SettingsPageTemplate" layer="default" handleinput="true">
            <Size>
                <AbsPoint x="616" y="576"/>
            </Size>
            <Anchors>
                <Anchor point="topright" relativePoint="topleft" relativeTo="$parentTabDivider">
                    <AbsPoint x="4" y="0"/>
                </Anchor>
            </Anchors>
            <Windows>
                <FullResizeImage name="$parentFrame" inherits="UO_Default_Inner_Window_Frame" layer="background" frameonly="false">
                    <Anchors>
                        <Anchor point="topleft" relativePoint="topleft" relativeTo="$parent"/>
                        <Anchor point="bottomright" relativePoint="bottomright" relativeTo="$parent" />
                    </Anchors>
                </FullResizeImage>
            </Windows>
        </Window>

        <Button name="SettingsTabTemplate" backgroundtexture="" highlighttexture="Menu_Selection" font="inter_medium_20" layer="default" textalign="center" texturescale="0.66">
            <Size>
                <AbsPoint x="128" y="36"/>
            </Size>
            <TexCoords>
                <NormalHighlit x="20" y="10"/>
                <PressedHighlit x="20" y="10"/>
            </TexCoords>
            <TextColors>
                <Normal r="206" g="217" b="242" a="255"/>
            </TextColors>
            <EventHandlers>
                <EventHandler event="OnLButtonUp" function="SettingsWindow.onTabClick"/>
            </EventHandlers>
        </Button>

        <Label name="SettingsLabelTemplate" autoresize="true" font="inter_medium_16">
            <Size>
                <AbsPoint x="150" y="18"/>
            </Size>
            <Color r="206" g="217" b="242" a="255"/>
        </Label>

        <Button name="SettingsCheckBoxTemplate" inherits="UO_Default_Check_Button">
            <Windows>
                <Label name="$parentLabel" inherits="SettingsLabelTemplate" textalign="left" autoresizewidth="true">
                    <Anchors>
                        <Anchor point="left" relativePoint="left">
                            <AbsPoint x="32" y="-2"/>
                        </Anchor>
                    </Anchors>
                </Label>
            </Windows>
            <EventHandlers>
                <EventHandler event="OnLButtonUp" function="SettingsWindow.onCheckBoxClicked"/>
            </EventHandlers>
        </Button>

        <Window name="SettingsSliderTemplate">
            <Size>
                <AbsPoint x="560" y="18"/>
            </Size>
            <Windows>
                <Label name="$parentLabel" inherits="SettingsLabelTemplate" wordwrap="true"/>

                <SliderBar name="$parentBar" inherits="UO_Default_Horizontal_SliderBar">
                    <Anchors>
                        <Anchor point="topright" relativePoint="topleft" relativeTo="$parentLabel">
                            <AbsPoint x="48" y="0" />
                        </Anchor>
                    </Anchors>
                    <EventHandlers>
                        <EventHandler event="OnSlide" function="SettingsWindow.onSliderChanged" />
                    </EventHandlers>
                </SliderBar>

                <Label name="$parentValue" inherits="SettingsLabelTemplate" wordwrap="true">
                    <Size>
                        <AbsPoint x="40" y="20"/>
                    </Size>
                    <Anchors>
                        <Anchor point="topright" relativePoint="topleft" relativeTo="$parentBar">
                            <AbsPoint x="20" y="0" />
                        </Anchor>
                    </Anchors>
                    <Color r="206" g="217" b="242" a="255"/>
                </Label>
            </Windows>
        </Window>

        <Window name="SettingsComboTemplate">
            <Size>
                <AbsPoint x="560" y="27"/>
            </Size>
            <Windows>
                <Label name="$parentLabel" inherits="SettingsLabelTemplate"/>
    
                <ComboBox name="$parentBox" inherits="UO_Default_LongComboBox">
                    <Anchors>
                        <Anchor point="right" relativePoint="left" relativeTo="$parentLabel">
                            <AbsPoint x="36" y="0"/>
                        </Anchor>
                    </Anchors>
                    <EventHandlers>
                        <EventHandler event="OnSelChanged" function="SettingsWindow.onComboBoxChanged"/>
                    </EventHandlers>
                </ComboBox>
            </Windows>
        </Window>

        <Window name="SettingsWindow" movable="true" layer="default">
            <Size>
                <AbsPoint x="800" y="600" />
            </Size>

            <Anchors>
                <Anchor point="center" relativePoint="center"/>
            </Anchors>
            <EventHandlers>
                <EventHandler event="OnInitialize" function="SettingsWindow.onInitialize"/>
                <EventHandler event="OnShutdown" function="SettingsWindow.onShutdown"/>
                <EventHandler event="OnRButtonUp" function="SettingsWindow.onRightClick"/>
            </EventHandlers>

            <Windows>
                <FullResizeImage name="$parentBackground" inherits="UO_Simple_Black_Background" layer="background">
                    <Anchors>
                        <Anchor point="topleft" relativePoint="topleft"/>
                        <Anchor point="bottomright" relativePoint="bottomright"/>
                    </Anchors>
                </FullResizeImage>
    
                <FullResizeImage name="$parentFrame" inherits="UO_Simple_Frame" layer="background" frameonly="false">
                    <Anchors>
                        <Anchor point="topleft" relativePoint="topleft" relativeTo="$parentBackground"/>
                        <Anchor point="bottomright" relativePoint="bottomright" relativeTo="$parentBackground" />
                    </Anchors>
                </FullResizeImage>

                <VerticalResizeImage name="$parentTabDivider" inherits="UO_Default_Vertical_Rule">
                    <Size>
                        <AbsPoint x="5" y="576" />
                    </Size>
                    <Anchors>
                        <Anchor point="topright" relativePoint="topleft" relativeTo="$parentTabContainer">
                            <AbsPoint x="18" y="12"/>
                        </Anchor>
                    </Anchors>
                </VerticalResizeImage>

                <Window name="$parentGraphics" inherits="GraphicsSettingsWindow"/>
                <Window name="$parentSound" inherits="SoundSettingsWindow"/>
                <Window name="$parentChat" inherits="ChatSettingsWindow"/>
                <Window name="$parentControlsPage" inherits="ControlsSettingsWindow"/>
                <Window name="$parentInterface" inherits="InterfaceSettingsWindow"/>

                <Window name="$parentTabContainer">
                    <Size>
                        <AbsPoint x="146" y="600"/>
                    </Size>
                    <Windows>
                        <Button name="$parentGraphicsTab" inherits="SettingsTabTemplate">
                            <Anchors>
                                <Anchor point="topleft" relativePoint="topleft">
                                    <AbsPoint x="18" y="36"/>
                                </Anchor>
                            </Anchors>
                        </Button>
        
                        <Button name="$parentSoundTab" inherits="SettingsTabTemplate">
                            <Anchors>
                                <Anchor point="bottomleft" relativePoint="topleft" relativeTo="$parentGraphicsTab"/>
                            </Anchors>
                        </Button>
        
                        <Button name="$parentChatTab" inherits="SettingsTabTemplate">
                            <Anchors>
                                <Anchor point="bottomleft" relativePoint="topleft" relativeTo="$parentSoundTab"/>
                            </Anchors>
                        </Button>

                        <Button name="$parentControlsTab" inherits="SettingsTabTemplate">
                            <Anchors>
                                <Anchor point="bottomleft" relativePoint="topleft" relativeTo="$parentChatTab"/>
                            </Anchors>
                        </Button>

                        <Button name="$parentInterfaceTab" inherits="SettingsTabTemplate">
                            <Anchors>
                                <Anchor point="bottomleft" relativePoint="topleft" relativeTo="$parentControlsTab"/>
                            </Anchors>
                        </Button>
                    </Windows>
                </Window>
            </Windows>
        </Window>

    </Windows>
</Interface>