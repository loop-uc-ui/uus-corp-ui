<Interface xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../Interface.xsd">
    <Windows>
        <Window name="GraphicsSettingsWindow" inherits="SettingsPageTemplate">
            <Windows>
                <FullResizeImage name="$parentFrame" inherits="UO_Default_Inner_Window_Frame" layer="background" frameonly="false">
                    <Anchors>
                        <Anchor point="topleft" relativePoint="topleft" relativeTo="$parent"/>
                        <Anchor point="bottomright" relativePoint="bottomright" relativeTo="$parent" />
                    </Anchors>
                </FullResizeImage>

                <Window name="$parentPage" layer="default">
                    <Size>
                        <AbsPoint x="580" y="540"/>
                    </Size>

                    <Anchors>
                        <Anchor point="topleft" relativePoint="topleft" relativeTo="$parentFrame">
                            <AbsPoint x="18" y="36"/>
                        </Anchor>
                        <Anchor point="bottomright" relativePoint="bottomright" relativeTo="$parentFrame">
                            <AbsPoint x="-36" y="36"/>
                        </Anchor>
                    </Anchors>

                    <Windows>
                        <Window name="$parentResCombo" inherits="SettingsComboTemplate"/>

                        <Button name="$parentFullScreenCheckBox" inherits="SettingsCheckBoxTemplate">
                            <Anchors>
                                <Anchor point="bottomleft" relativePoint="topleft" relativeTo="$parentResCombo">
                                    <AbsPoint x="0" y="18"/>
                                </Anchor>
                            </Anchors>
                        </Button>

                        <Button name="$parentFlyingAnimationCheckBox" inherits="SettingsCheckBoxTemplate">
                            <Anchors>
                                <Anchor point="bottomleft" relativePoint="topleft" relativeTo="$parentFullScreenCheckBox">
                                    <AbsPoint x="0" y="18"/>
                                </Anchor>
                            </Anchors>
                        </Button>

                        <Button name="$parentWindowFrameCheckBox" inherits="SettingsCheckBoxTemplate">
                            <Anchors>
                                <Anchor point="bottomleft" relativePoint="topleft" relativeTo="$parentFlyingAnimationCheckBox">
                                    <AbsPoint x="0" y="18"/>
                                </Anchor>
                            </Anchors>
                        </Button>
    
                        <Button name="$parentHouseContentsCheckBox" inherits="SettingsCheckBoxTemplate">
                            <Anchors>
                                <Anchor point="bottomleft" relativePoint="topleft" relativeTo="$parentWindowFrameCheckBox">
                                    <AbsPoint x="0" y="18"/>
                                </Anchor>
                            </Anchors>
                        </Button>
    
                        <Button name="$parentDeathEffectCheckBox" inherits="SettingsCheckBoxTemplate">
                            <Anchors>
                                <Anchor point="bottomleft" relativePoint="topleft" relativeTo="$parentHouseContentsCheckBox">
                                    <AbsPoint x="0" y="18"/>
                                </Anchor>
                            </Anchors>
                        </Button>
    
                        <Button name="$parentVSyncCheckBox" inherits="SettingsCheckBoxTemplate">
                            <Anchors>
                                <Anchor point="bottomleft" relativePoint="topleft" relativeTo="$parentDeathEffectCheckBox">
                                    <AbsPoint x="0" y="18"/>
                                </Anchor>
                            </Anchors>
                        </Button>
    
                        <Button name="$parentIdleAnimationCheckBox" inherits="SettingsCheckBoxTemplate">
                            <Anchors>
                                <Anchor point="bottomleft" relativePoint="topleft" relativeTo="$parentVSyncCheckBox">
                                    <AbsPoint x="0" y="18"/>
                                </Anchor>
                            </Anchors>
                        </Button>
    
                        <Button name="$parentFoliageCheckBox" inherits="SettingsCheckBoxTemplate">
                            <Anchors>
                                <Anchor point="bottomleft" relativePoint="topleft" relativeTo="$parentIdleAnimationCheckBox">
                                    <AbsPoint x="0" y="18"/>
                                </Anchor>
                            </Anchors>
                        </Button>
    
                        <Button name="$parentShadowsCheckBox" inherits="SettingsCheckBoxTemplate">
                            <Anchors>
                                <Anchor point="bottomleft" relativePoint="topleft" relativeTo="$parentFoliageCheckBox">
                                    <AbsPoint x="0" y="18"/>
                                </Anchor>
                            </Anchors>
                        </Button>
    
                        <Button name="$parentCircleOfTransparencyCheckBox" inherits="SettingsCheckBoxTemplate">
                            <Anchors>
                                <Anchor point="bottomleft" relativePoint="topleft" relativeTo="$parentShadowsCheckBox">
                                    <AbsPoint x="0" y="18"/>
                                </Anchor>
                            </Anchors>
                        </Button>

                        <Window name="$parentBrightnessSlider" inherits="SettingsSliderTemplate">
                            <Anchors>
                                <Anchor point="bottomleft" relativePoint="topleft" relativeTo="$parentCircleOfTransparencyCheckBox">
                                    <AbsPoint x="0" y="18"/>
                                </Anchor>
                            </Anchors>
                        </Window>

                        <Window name="$parentParticleDetailCombo" inherits="SettingsComboTemplate">
                            <Anchors>
                                <Anchor point="bottomleft" relativePoint="topleft" relativeTo="$parentBrightnessSlider">
                                    <AbsPoint x="0" y="18"/>
                                </Anchor>
                            </Anchors>
                        </Window>

                        <Window name="$parentParticleFilterCombo" inherits="SettingsComboTemplate">
                            <Anchors>
                                <Anchor point="bottomleft" relativePoint="topleft" relativeTo="$parentParticleDetailCombo">
                                    <AbsPoint x="0" y="18"/>
                                </Anchor>
                            </Anchors>
                        </Window>
                    </Windows>
                </Window>
            </Windows>
        </Window>
    </Windows>
</Interface>