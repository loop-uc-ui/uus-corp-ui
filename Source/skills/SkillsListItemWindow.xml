<Interface xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../Interface.xsd">
    <Scripts>
        <Script file="SkillsListItemWindow.lua"/>
    </Scripts>
    <Windows>
        <Window name="SkillsListItemWindow" layer="overlay" handleinput="true">
            <Size>
                <AbsPoint x="512" y="22"/>
            </Size>
            <Windows>
                <Button name="$parentButtonBackground" texturescale="0.9" backgroundtexture="" highlighttexture="Menu_Selection"
                        font="UO_GoldButtonText" layer="default">
                    <Size>
                        <AbsPoint x="195" y="45"/>
                    </Size>
                    <Anchors>
                        <Anchor point="topleft" relativePoint="topleft">
                            <AbsPoint x="0" y="-10"/>
                        </Anchor>
                        <Anchor point="bottomright" relativePoint="bottomright" relativeTo="$parentLockButtonWindow">
                            <AbsPoint x="0" y="10"/>
                        </Anchor>
                    </Anchors>
                    <TexCoords>
                        <NormalHighlit x="10" y="10"/>
                        <PressedHighlit x="10" y="10"/>
                    </TexCoords>
                </Button>

                <Label name="$parentName" inherits="UO_Default_Label_Color" font="body"
                       handleinput="false" autoresize="true" textalign="left">
                    <Size>
                        <AbsPoint x="162" y="22"/>
                    </Size>
                    <Anchors>
                        <Anchor point="topleft" relativePoint="topleft" relativeTo="$parent">
                            <AbsPoint x="0" y="0"/>
                        </Anchor>
                    </Anchors>
                </Label>

                <Label name="$parentRealValue" inherits="UO_Default_Label_Color" font="body"
                       handleinput="false" autoresize="true" textalign="right">
                    <Size>
                        <AbsPoint x="76" y="22"/>
                    </Size>
                    <Anchors>
                        <Anchor point="topright" relativePoint="topleft" relativeTo="$parentName">
                            <AbsPoint x="10" y="0"/>
                        </Anchor>
                    </Anchors>
                </Label>

                <Label name="$parentModifiedValue" inherits="UO_Default_Label_Color" font="body"
                       handleinput="false" autoresize="true" textalign="right">
                    <Size>
                        <AbsPoint x="76" y="22"/>
                    </Size>
                    <Anchors>
                        <Anchor point="topright" relativePoint="topleft" relativeTo="$parentRealValue">
                            <AbsPoint x="10" y="0"/>
                        </Anchor>
                    </Anchors>
                </Label>

                <Label name="$parentCapValue" inherits="UO_Default_Label_Color" font="body"
                       handleinput="false" autoresize="true" textalign="right">
                    <Size>
                        <AbsPoint x="76" y="22"/>
                    </Size>
                    <Anchors>
                        <Anchor point="topright" relativePoint="topleft" relativeTo="$parentModifiedValue">
                            <AbsPoint x="10" y="0"/>
                        </Anchor>
                    </Anchors>
                </Label>

                <Window name="$parentLockButtonWindow" layer="overlay">
                    <Size>
                        <AbsPoint x="22" y="22" />
                    </Size>
                    <Anchors>
                        <Anchor point="right" relativePoint="left" relativeTo="$parentCapValue">
                            <AbsPoint x="0" y="0" />
                        </Anchor>
                    </Anchors>
                </Window>
            </Windows>
        </Window>
    </Windows>
</Interface>