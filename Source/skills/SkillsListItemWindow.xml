        <Script file="SkillsListItemWindow.lua"/>
    </Scripts>
    <Windows>
        <Window name="SkillsListItemWindow" layer="overlay" handleinput="true">
            <Size>
                <AbsPoint x="512" y="22"/>
            </Size>
            <Windows>
                <DynamicImage name="$parentBackground" layer="background" handleinput="true" texture="UO_Core">
                    <Anchors>
                        <Anchor point="topleft" relativePoint="topleft" relativeTo="$parentName">
                            <AbsPoint x="0" y="-6"/>
                        </Anchor>
                        <Anchor point="bottomright" relativePoint="bottomright" relativeTo="$parentCapValue"/>
                    </Anchors>
                    <Size>
                        <AbsPoint x="76" y="22"/>
                    </Size>
                    <TexCoords>
                        <TopLeft x="260" y="4" />
                        <TopCenter x="263" y="4" />
                        <TopRight x="313" y="4" />
                        <MiddleLeft x="260" y="4" />
                        <MiddleCenter x="263" y="4" />
                        <MiddleRight x="313" y="4" />
                        <BottomLeft x="260" y="63" />
                        <BottomCenter x="263" y="63" />
                        <BottomRight x="313" y="63" />
                    </TexCoords>
                </DynamicImage>

                <Label name="$parentName" inherits="UO_Default_Label_Color" font="body"
                       handleinput="false" autoresize="true" textalign="left">
                    <Size>
                        <AbsPoint x="162" y="22"/>
                    </Size>
                    <Anchors>
                        <Anchor point="topleft" relativePoint="topleft" relativeTo="$parent">
                            <AbsPoint x="0" y="0"/>
                        </Anchor>
                    </Anchors>
                </Label>

                <Label name="$parentRealValue" inherits="UO_Default_Label_Color" font="body"
                       handleinput="false" autoresize="true" textalign="right">
                    <Size>
                        <AbsPoint x="76" y="22"/>
                    </Size>
                    <Anchors>
                        <Anchor point="topright" relativePoint="topleft" relativeTo="$parentName">
                            <AbsPoint x="10" y="0"/>
                        </Anchor>
                    </Anchors>
                </Label>

                <Label name="$parentModifiedValue" inherits="UO_Default_Label_Color" font="body"
                       handleinput="false" autoresize="true" textalign="right">
                    <Size>
                        <AbsPoint x="76" y="22"/>
                    </Size>
                    <Anchors>
                        <Anchor point="topright" relativePoint="topleft" relativeTo="$parentRealValue">
                            <AbsPoint x="10" y="0"/>
                        </Anchor>
                    </Anchors>
                </Label>

                <Label name="$parentCapValue" inherits="UO_Default_Label_Color" font="body"
                       handleinput="false" autoresize="true" textalign="right">
                    <Size>
                        <AbsPoint x="76" y="22"/>
