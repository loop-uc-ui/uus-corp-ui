                <Window name="$parentLockButtonWindow" layer="overlay">
                    <Size>
                        <AbsPoint x="22" y="22" />
                    </Size>
                    <Anchors>
                        <Anchor point="right" relativePoint="left" relativeTo="$parentCapValue">
                            <AbsPoint x="0" y="0" />
                        </Anchor>
                    </Anchors>
                </Window>
            </Windows>
        </Window>
    </Windows>
</Interface>