<?xml version="1.0" encoding="UTF-8"?>
<Interface xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../Interface.xsd">
    <Windows>
        <Button name="ItemGridSocketTemplate" draganddrop="true" texturescale="1.0">
            <Size>
                <AbsPoint x="51" y="51" />
            </Size>
            <Windows>
                <DynamicImage name="$parentIcon" texturescale="1.0" handleinput="false" layer="default">
                    <Size>
                        <AbsPoint x="48" y="48" />
                    </Size>
                    <Anchors>
                        <Anchor point="center" relativePoint="center" relativeTo="$parent"/>
                    </Anchors>
                </DynamicImage>
                <DynamicImage name="$parentIconMulti" texturescale="1.0" handleinput="false" layer="secondary">
                    <Size>
                        <AbsPoint x="48" y="48" />
                    </Size>
                    <Anchors>
                        <Anchor point="center" relativePoint="center" relativeTo="$parentIcon">
                            <AbsPoint x="6" y="6" />
                        </Anchor>
                    </Anchors>
                </DynamicImage>
                <Label name="$parentQuantity" font="UO_DefaultText_15pt" maxchars="6" textalign="left" autoresize="true" wordwrap="false" layer="popup" handleinput="false">
                    <Size>
                        <AbsPoint x="60" y="15" />
                    </Size>
                    <Anchors>
                        <Anchor point="bottomleft" relativePoint="bottomleft">
                            <AbsPoint x="2" y="-2" />
                        </Anchor>
                    </Anchors>
                    <Color r="255" g="255" b="255" a="255" />
                </Label>
            </Windows>
            <TexCoords>
                <Normal texture="hud_image" x="412" y="971" />
                <NormalHighlit texture="hud_image" x="462" y="971" />
                <Disabled texture="hud_image" x="412" y="971" />
                <Pressed texture="hud_image" x="412" y="971" />
                <PressedHighlit texture="hud_image" x="462" y="971" />
            </TexCoords>
        </Button>
    </Windows>
</Interface>